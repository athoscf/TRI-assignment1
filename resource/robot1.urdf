<?xml version="1.0" ?>
<robot name="Robot 1">
    <webots>
        <device reference="ds0" type="DistanceSensor">
            <ros>
                <topicName>/robot1/left_sensor</topicName>
                <alwaysOn>true</alwaysOn>
            </ros>
        </device>
        <device reference="ds1" type="DistanceSensor">
            <ros>
                <topicName>/robot1/right_sensor</topicName>
                <alwaysOn>true</alwaysOn>
            </ros>
        </device>
        <plugin type="my_robot_driver::MyRobotDriver" />
    </webots>
</robot>